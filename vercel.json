{
  "version": 2,
  "builds": [
    {
      "src": "garcode_project/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "garcode_project/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "PYTHONUNBUFFERED": "true"
    },
    "commands": {
      "build": "pip install -r requirements.txt"
    }
  }
}
